# Uncomment this line to define a global platform for your project
# platform :ios, '9.0'

target 'Chilindo' do
    # Comment this line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!
    
    ###AutoLayout
    pod 'Cartography'
    
    ###Image downloader
    pod 'Kingfisher'
    
    ###Network
    pod 'Alamofire'
    pod 'RxAlamofire'
    pod 'AlamofireObjectMapper'
    
    ###Json Parse
    pod 'ObjectMapper'
    
    ###Rxswift
    pod 'RxSwift'
    
    ###Messages
    pod 'SwiftMessages'
        
    target 'ChilindoTests' do
        inherit! :search_paths
        # Pods for testing
    end
    
end
